{
  "name": "ffmpegx",
  "version": "0.0.1",
  "description": "Using FFMPEG with js",
  "main": "dist/main.js",
  "module": "dist/main.mjs",
  "types": "dist/main.d.ts",
  "exports": {
    ".": {
      "import": "./dist/main.mjs",
      "require": "./dist/main.js"
    },
    "./src/main.ts": {
      "default": "./src/main.ts"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "tsup --minify",
    "test": "jest",
    "dev": "tsx watch",
    "prepublish": "pnpm build"
  },
  "files": [
    "dist",
    "src"
  ],
  "keywords": [
    "ffmpeg"
  ],
  "author": "lenml",
  "license": "AGPL-3.0-only",
  "publishConfig": {
    "access": "public"
  },
  "packageManager": "pnpm@10.12.4",
  "devDependencies": {
    "@microsoft/api-extractor": "^7.52.11",
    "@types/node": "^24.3.1",
    "tslib": "^2.8.1",
    "tsup": "^8.5.0",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2"
  },
  "dependencies": {
    "@quik-fe/flick": "^1.0.1",
    "@types/which": "^3.0.4",
    "eventemitter3": "^5.0.1",
    "ffmpegx-core": "workspace:*",
    "which": "^5.0.0",
    "zod": "^4.1.5"
  }
}
